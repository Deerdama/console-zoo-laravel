{
  "name": "deerdama/console-zoo",
  "version": "v1",
  "description": "package to easily add styles and icons into console outputs",
  "require": {
    "php": ">=7.1"
  },
  "autoload": {
    "psr-4": {
      "Deerdama\\ConsoleZoo\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Deerdama\\ConsoleZoo\\ConsoleZooServiceProvider"
      ]
    }
  },
  "authors": [
    {
      "name": "Deerdama"
    }
  ],
  "keywords": [
    "console",
    "icons",
    "colors",
    "command",
    "output",
    "laravel",
    "symfony",
    "artisan"
  ]
}